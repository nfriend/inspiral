{
  "name": "@nfriend/inspiral-gear-generator",
  "version": "1.0.0",
  "description": "A utility to generate gears for Inspiral",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "yarn clean && yarn svg:all && yarn render && yarn dart:fmt-generated && yarn png:compress",
    "render": "ts-node src/index.ts",
    "svg:circles": "ts-node src/svg_automation/circles.ts",
    "svg:ovals": "ts-node src/svg_automation/ovals.ts",
    "svg:polygons": "ts-node src/svg_automation/polygons.ts",
    "svg:all": "yarn svg:circles && yarn svg:ovals && yarn svg:polygons",
    "clean": "ts-node src/clean.ts",
    "dart:fmt-generated": "ts-node src/format_generated_dart_files.ts",
    "png:compress": "ts-node src/compress_png.ts",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@types/ejs": "3.0.5",
    "@types/glob": "7.1.3",
    "@types/lodash.range": "^3.2.6",
    "@types/node": "14.14.20",
    "@types/puppeteer": "5.4.2",
    "camelcase": "6.2.0",
    "chalk": "4.1.0",
    "ejs": "3.1.5",
    "glob": "7.1.6",
    "lodash.range": "^3.2.0",
    "puppeteer": "5.5.0"
  }
}
