<?xml version="1.0"?>
<svg xmlns="http://www.w3.org/2000/svg"
     width="<%= width %>"
     height="<%= height %>"
     viewbow="0 0 <%= width %> <%= height %>">
  <style>
    #gear {
      fill:#000000;
      fill-opacity:0.3;
      stroke:#000000;
      stroke-width: <%= 0.250 * baseScale %>;
    }
  </style>
  <g>
    <path id="gear"
          fill-rule="evenodd"
          d="<%= gearPath.toString() %>
             <%_ for (const holePath of holePaths) { _%>
             <%= holePath.toString() %>
             <%_ } _%>
            " />
  </g>
</svg>
