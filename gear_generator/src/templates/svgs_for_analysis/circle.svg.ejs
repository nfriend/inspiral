<?xml version="1.0"?>
<svg xmlns="http://www.w3.org/2000/svg" xmlns:inspiral="http://nathanfriend.io/inspiral">
  <desc xmlns:inspiral="http://nathanfriend.io/inspiral">
    <gear-order>
      <%= gearOrder %>
    </gear-order>
    <is-round>
      true
    </is-round>
    <%_ if (isRing) { _%>
    <inverted>
      true
    </inverted>
    <%_ } _%>
  </desc>
  <g>
    <%- include('gear_holes') %>
    <path mask="url(#holes)"
          d="M <%= radius * 2 %> <%= radius %>
             A <%= radius %> <%= radius %> 0 1 0 0 <%= radius %>
             A <%= radius %> <%= radius %> 0 0 0 <%= radius * 2%> <%= radius %>
             Z" />
  </g>
</svg>
