<?xml version="1.0"?>
<svg xmlns="http://www.w3.org/2000/svg" xmlns:inspiral="http://nathanfriend.io/inspiral">
  <desc xmlns:inspiral="http://nathanfriend.io/inspiral">
    <gear-order>
      <%= gearOrder %>
    </gear-order>
    <is-round>
      true
    </is-round>
    <%_ if (isRing) { _%>
    <inverted>
      true
    </inverted>
    <%_ } _%>
  </desc>
  <g>
    <%- include('gear_holes') %>
    <path id="shape"
          mask="url(#holes)"
          d="M <%= radius * 2 %> <%= radius %>
             A <%= radius %> <%= radius %> 0 1 0 0 <%= radius %>
             A <%= radius %> <%= radius %> 0 0 0 <%= radius * 2%> <%= radius %>
             Z" />

    <%- include('clip_style') %>

    <%_ if (isRing) { _%>
    <ellipse id="clip"
             cx="<%= radius %>"
             cy="<%= radius %>"
             rx="<%= radius - toothHeight * 2.5 %>"
             ry="<%= radius - toothHeight * 2.5 %>" />
    <%_ } else { _%>
    <ellipse id="clip"
             cx="<%= radius %>"
             cy="<%= radius %>"
             rx="<%= radius %>"
             ry="<%= radius %>" />
    <%_ } _%>
  </g>
</svg>
