<?xml version="1.0"?>
<svg xmlns="http://www.w3.org/2000/svg" xmlns:inspiral="http://nathanfriend.io/inspiral">
  <desc xmlns:inspiral="http://nathanfriend.io/inspiral">
    <gear-order>
      <%= gearOrder %>
    </gear-order>
  </desc>
  <style>
    text {
      font-family: monospace;
      font-size: 4px;
    }
  </style>
  <g>
    <mask id="holes">
			<rect width="100%" height="100%" fill="white" />
      <%_ for (const hole of holes) { _%>
			<circle cx="<%= hole.point.x %>"
              cy="<%= hole.point.y %>"
              r="<%= holeSize %>"
              inspiral:hole-name="<%= hole.name %>"
              inspiral:hole-text-position-angle="<%= hole.textPositionAngle %>"
              inspiral:hole-text-rotation-angle="<%= hole.textRotationAngle %>" />
      <text x="<%= hole.point.x - 3 %>" y="<%= hole.point.y + 1 %>" text-anchor="end"><%= hole.name %></text>
      <%_ } _%>
		</mask>
    <path mask="url(#holes)"
          d="M <%= radius * 2 %> <%= radius %>
             A <%= radius %> <%= radius %> 0 1 0 0 <%= radius %>
             A <%= radius %> <%= radius %> 0 0 0 <%= radius * 2%> <%= radius %>
             Z" />
  </g>
</svg>
