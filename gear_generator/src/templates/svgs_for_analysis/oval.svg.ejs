<?xml version="1.0"?>
<svg xmlns="http://www.w3.org/2000/svg" xmlns:inspiral="http://nathanfriend.io/inspiral">
  <desc xmlns:inspiral="http://nathanfriend.io/inspiral">
    <entitlement-id>
      io.nathanfriend.inspiral.ovalgears
    </entitlement-id>
    <package-id>
      io.nathanfriend.inspiral.ovalgears
    </package-id>
    <gear-order>
      <%= gearOrder %>
    </gear-order>
    <%_ if (isRing) { _%>
    <inverted>
      true
    </inverted>
    <%_ } _%>
  </desc>
  <g>
    <%- include('gear_holes') %>
    <path id="shape"
          mask="url(#holes)"
          d="M <%= xRadius * 2 %> <%= yRadius %>
             A <%= xRadius %> <%= yRadius %> 0 1 0 0 <%= yRadius %>
             A <%= xRadius %> <%= yRadius %> 0 0 0 <%= xRadius * 2 %> <%= yRadius %>
             Z" />

    <%- include('clip_style') %>

    <%_ if (isRing) { _%>
    <ellipse id="clip"
             cx="<%= xRadius %>"
             cy="<%= yRadius %>"
             rx="<%= xRadius - toothHeight * 2.5 %>"
             ry="<%= yRadius - toothHeight * 2.5 %>" />
    <%_ } else { _%>
    <ellipse id="clip"
             cx="<%= xRadius %>"
             cy="<%= yRadius %>"
             rx="<%= xRadius %>"
             ry="<%= yRadius %>" />
    <%_ } _%>
  </g>
</svg>
